@page "/register"
<PageTitle>Register</PageTitle>
<link href="css/Register.css" rel="stylesheet" />
<p>Welcome!</p>
<body>
    <form class="reg-menu" action="/account">
        <input type="text" placeholder="First Name" name="fname" required>   <input type="text" placeholder="Last Name" name="lname" required><br><br>
        <input type="text" placeholder="Email" name="email" required>   <input type="text" placeholder="Re-enter Email" name="email" required><br><br>
        <input type="password" placeholder="Password" name="pswd" required>   <input type="password" placeholder="Re-enter Password" name="pswd" required><br><br>
        <button class="btn" type="submit">Go</button><br><br>
    </form>
</body>
